{
    "name": "codestream",
    "displayName": "CodeStream",
    "description": "CodeStream - Chat about your code along side your code",
    "version": "0.0.1",
    "publisher": "CodeStream",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.onCodeStream"
    ],
    "icon": "images/cs-icon-black-128x128.png",
    "galleryBanner": {
        "color": "#000000",
        "theme": "light"
    },
    "main": "./extension",
    "homepage": "https://codestream.com/",
    "contributes": {
        "commands": [
            {
                "command": "extension.onCodeStream",
                "title": "Enable CodeStream"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
